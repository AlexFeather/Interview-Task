<Window x:Class="VM.DeleteConfirm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VM"
        mc:Ignorable="d"
        Title="Delete Confirm" Height="100" Width="400">
    <Grid Margin="5,5,5,5">
        <TextBlock HorizontalAlignment="Center" Margin="0,15,0,0" Text="Delete element?" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <Button x:Name="Ok_Button" Content="Ok" HorizontalAlignment="Left" Margin="95,0,0,15" VerticalAlignment="Bottom" Width="40" Click="OkButton_Click"/>
        <Button x:Name="Cancel_Button" Content="Cancel" HorizontalAlignment="Right" Margin="0,0,95,15" VerticalAlignment="Bottom" Width="40" Click="CancelButton_Click"/>
    </Grid>
</Window>
